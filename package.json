  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SHINDongHyeo/random-korean-nickname/issues"
  },
  "homepage": "https://github.com/SHINDongHyeo/random-korean-nickname#readme",
  "devDependencies": {
    "typescript": "^5.6.3"
  },
  "exports": {
    ".": {
